{
  "version": 2,
  "builds": [
    {
      "src": "blog/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "500mb",
        "runtime": "python3.11"
      }
    }
  ],
  "buildCommand": "python manage.py collectstatic --noinput",
  "routes": [
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "blog/wsgi.py"
    }
  ]
}
